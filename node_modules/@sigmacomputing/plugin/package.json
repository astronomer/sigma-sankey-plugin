{
  "name": "@sigmacomputing/plugin",
  "version": "0.6.54",
  "description": "Plugin client api",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist tsconfig.tsbuildinfo",
    "build": "yarn run clean && tsc --build",
    "build:watch": "yarn run build --watch",
    "lint": "eslint --cache src --ext ts",
    "format": "prettier --write '{src,test}/**/*.ts'",
    "test": "jest --testPathPattern='\\.test\\.ts'",
    "prepublish": "yarn run build"
  },
  "jest": {
    "roots": [
      "src/"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    }
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0"
  },
  "eslintConfig": {
    "extends": "../../config/eslintrc.typescript.json"
  },
  "gitHead": "ddfd7ff94ba6d5a9610529285b8d68ea99fc8ba6"
}
